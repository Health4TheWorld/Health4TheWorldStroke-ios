/* FAQ */
"Faq.questionOne" = "Ano ang stroke?";
"Faq.answerOne" = "Sa stroke, ang suplay ng dugo sa isang partikular na bahagi ng utak ay nabarahan o ang mga saluyan ng dugo na nagsusuplay sa isang bahagi ng utak ay pumutok.  Nagreresulta ito ng pagkamatay ng cell sa apektadong bahagi ng utak.  Maaaring magresulto ito ng pansamantala o permanenteng pagkawala ng pagpapaandar ng isa o higit pa na bahagi ng katawan.";
"Faq.questionTwo" = "Ano ang dap at mong gawin kung sa tingin mo ang isang tao ay nakakaranas ng stroke?";
"Faq.answerTwo" =  "Agad na tumawag ng propesyunal na tulong medikal (doktor/ nars).";
"Faq.questionThree" = "Bakit kailangan mong mabilis na tumugon?";
"Faq.answerThree" = "Ang mabilis na paglapat ng medikal na gamutan ay nakakapagpababa ng panganib na maging kapansanan o maging kamatayan.";

/* BLOOD PRESSURE */
"BloodPressure.problem" = "Ang mataas na presyon sa dugo ay maaaring magdulot ng stroke na makakasira sa mga ugat sa buong katawan, na magiging sanhi ng pagputok o mas madaling pagbara.";
"BloodPressure.problemBoldPart" = "Mataas na presyon ng dugo";
"BloodPressure.problemNonBoldPart" = "maaaring maging sanhi ng stroke sa pagkasira ng mga ugat sa buong katawan, magiging sanhi ng pagputok o mas madaling pagbara.";
"BloodPressure.whatToDo" = "Sa pangkalahatang populasyon na may edad 60 taon o mas matanda, ang paggamot sa BP na layuning maibaba kaysa 150/90 mm Hg ay nakakabawas ng stroke.";
"BloodPressure.whatToDoBoldPart" = "mas mababa sa 150/90 mm Hg";

/* HEART RATE */
"HeartRate.problem" = "Atrial Fibrillation, o hindi regular na pintig ng puso, ay isang panganiib na dahilan para sa stroke.";
"HeartRate.problemBoldPart" = "Atrial Fibrillation";
"HeartRate.whatToDo" = "Kung kayo ay may hinala ng hindi regular na pintig ng puso, kontakin ang iyong doktor kaagad para masuri, na maaaring may kasamang electrocardiogram (ECG/EKG).  Base sa iyong nakalipas na kasaysayang medikal at ang iyong panganib sa pagkakaroon ng stroke sa hinaharap, may ilang mga opsyon para sa mga gamot, kagaya ng anticoagulants, na makakatulong na pamahalaan ang Afib.";
"HeartRate.whatToDoFirstBoldPart" = "electrocardiogram";
"HeartRate.whatToDoSecondBoldPart" = "anticoagulants";

/* BLOOD SUGAR */
"BloodSugar.problem" = "Ang mataas na presyon ng dugo ay makakadagdag sa panganib sa pagbubuo ng stroke at sakit sa puso.  Ang isang pagsusuri sa dugo na tinatawag na HbA1c ay makapagbibgay ng larawan para sa iyong karaniwang pagkontrol sa blood sugar sa nakalipas na 2 hanggang 3 buwan.";
"BloodSugar.problemBoldPart" = "Mataas na blood sugar";
"BloodSugar.whatToDo" = "Nirerekomenda ng American Heart Association ang pagpapanatili ng antas ng blood sugar (Hgb1A1c) na mababa sa 6% para sa mga pasyenteng may pre-diabetes at mababa sa 7% para sa mga pasyenteng may diabetes.";
"BloodSugar.whatToDoFirstBoldPart" = "mababa sa 6%";
"BloodSugar.whatToDoSecondBoldPart" = "mababa sa 7%";

/* DIET */
"Diet.problem" = "Importante sa mga pasyente ng stroke na mapanatili ang malusog na pamumuhay.  Ang mga pasyente ng stroke na nakatira sa mga pangmatagalang pangangalagang pasilidad ay nasa panganib ng kakulangan sa calcium at bitamina D.";
"Diet.whatToDo" = "Mas mabuti ang pagkain ng pangkalahatang malusog na diyeta kasama ang iba’t ibang mga prutas at gulay, buong butil, mga produktong gatas na mababa ang taba, walang balat na mga karne at isda, at mga mani at mga butong gulay.  Magbawas sa mga saturated fat, trans fat, sodium, karneng may mapupulang laman, matatamis at mga inuming pinatamis ng asukal.";

/* BLADDER/BOWEL */
"BladderBowel.problem" = "May ibang mga pasyente ang makakaranas ng kawalan ng pagpigil ng ihi o pagpapanatili.";
"BladderBowel.whatToDoBulletOne" = "Tantiyahin ang pagpapanatili ng ihi sa pamamagitan ng pag-scan ng pantog o paulit-ulit na pagkalilya matapos ang kawalan ng saysay, at maramihang tala.";
"BladderBowel.whatToDoBulletTwo" = "Regular na magpunta sa banyo para masanay ang inyong pantog.  Ang pag-ihi kada 2-3 oras ay makakatulong upang maiwasan ang mga aksidente.";
"BladderBowel.whatToDoBulletThree" = "Uminom ng maraming likido sa buong araw para mapanatiling hydrated para matulungan kayong maiwasan ang paninigas ng dumi.";


/* EXERCISES */
"Exercises.problem" = "Ang mga pasyente ng stroke ay kadalasang nagkakaroon ng paghihirap maibalik ang pisikal na lakas at kakayahan.";
"Exercises.parOne" = "Ang mga nakaligtas sa stroke ay dapat makatanggap ng rehabilitisasyon at ang tindi ay kailangang tumugma kasama ang benepisyo at kayang tiisin.";
"Exercises.bulletOne" = "Para makatulong sa limitasyon sa paglalakad: Kailangang gumawa ang mga pasyente ng matindi, pauli-ulit, pagsasanay sa mga gawaing-panggalaw sa ilalim ng pangangasiwa ng isang propesyunal sa pangkalusugang pangangalaga.";
"Exercises.bulletOneBoldPart" = "mga limitasyon sa paglalakad";
"Exercises.bulletTwo" = "Para makatulong sa pagkakahulog: Ang mga pasyente ay kailangang gumawa ng mga pagsasanay sa pagbabalanse.";
"Exercises.bulletTwoBoldPart" = "nahuhulog:";
"Exercises.bulletThree" = "Para makatulog sa pagkaka-ospital at maagang paggaling: Iminumungkahi ng AHA/ASA ang mababang antas ng paglalakad, sariling pangangalagang gawain, paulit-ulit na upo o tayo, mga gawaing nakaupo, o iba pang mga gumagalaw na mga gawain.  Ang mga gawaing ito ay kailangang gawin na may panuntunan para makuha ang 10 hanggang 20 pintig na pagtaas kada minuto ng pintig ng puso, depende sa mungkahi ng inyong doktor.";
"Exercises.bulletThreeBoldPart" = "pagka-ospital at maagang paggaling ";
"Exercises.bulletFour" = "Para makatulong sa ga panloob at panlabas ng mga pasyente sa kanilang ehersisyong gamutan o rehabilitisasyon: Iminumungkahi ng AHA/ASA ang mga gawain gaya ng paglalakad ng 3-5 araw bawat linggo ng may 20-60 min/ sesyon (o maraming tig 10-min mga sesyon); pag-uunat ng 2-3 araw sa isang linggo sa loob ng 10-30 segundo; Tai chi, yoga, at mga gawaing panlibangan bilang pantulong sa paglalakad o pag-uunat.";
"Exercises.bulletFourBoldPart" = "rehabilitisasyon at patuloy na pag-ehersisyo para sa panloob o panlabas na pasyente";
"Exercises.bulletFive" = "Para manatiling ligtas sa twing magkakarron ng pisikal na ehersisyo:  Gumamit ng mga gamit na makakatulong sa paglalakad at mag-ingat sa pag-eehersisyo, sa ilalim ng nangangasiwa.";
"Exercises.bulletFiveBoldPart" = "ligtas tuwing magpipisikal na ehersisyo";
"Exercises.bulletSix" = "Para mapanatili ang pisikal na kalusugan: Isangguni sa inyong doktor para sa natatanging pisikal na gawain na gusto, partikular na limitasyon sa mga gawain, at ang mga paghihigpit sa pakikilahok sa pag-eehersisyo.";
"Exercises.bulletSixBoldPart" = "pisikal na kalusugan:";


/* SHOWER/BATH */
"ShowerBath.problem" = "Pagpasok at paglabas sa bathtub/ paliguan ay maaaring mapanganib, at may panganib na mahulog ang pasyente.";
"ShowerBath.bulletOne" = "Paliguan > tubs: Sa pangkalahatan ang mga paliguan ay mas ligtas kaysa sa mga tubs.";
"ShowerBath.bulletOneBoldPart" = "Mga Paliguan > tubs:";
"ShowerBath.bulletTwo" = "Ang mga grab bar ay maaaring makatulong para mas madaling kumilos.";
"ShowerBath.bulletTwoBoldPart" = "Mga grab bar";
"ShowerBath.bulletThree" = "Ang mga alpombra sa paliguan na may suction cups ay makakatulong para maiwasan ang pagkadulas.";
"ShowerBath.bulletThreeBoldPart" = "Mga alpombra sa paliguan";
"ShowerBath.paragraph" = "Siguruhin na may taong malapit na pwede kang tulungan kung kinakailangan.  Panatilihin ang kampanilya sa tabi mo o magkaroon ng ibang paraan para humingi ng tulong.  Ang pagsasanay sa pagbabalanse ay makakatulong sa mga indibidwal na may problema sa pagbabalanse.";

/* HOME MOD */
"HomeMod.problem" = "Dahil ang mga pasyente ay maaaring makaanas ng limitadong paggalaw, mahalaga na ayusin ang inyong bahay upang it ay ligtas at magagalawan ng maayos.";
"HomeMod.subHeaderOne" = "Kwarto:";
"HomeMod.contentOne" = "Ang paggalaw sa kwarto ay maaaring mapabuti sa paglipat sa mga bagay upang ang pinto ay mabuksan ng mabuti; gawing malapad ang mga pintuan; ilipat ang mga bisagra upang ang pinto ay magbukas palabas sa halip na sa loob.  Kung ang kama ay kailangang nakasandal sa pader, ito ay dapat i-ayon sa apektadong bahagi ng pasyente (mahinang bahagi) malapit s apader, upang ang malakas nabahagi ay makatulong sa pasyente sa paglabas sa kama.";
"HomeMod.subHeaderTwo" = "Heneral:";
"HomeMod.contentTwo" = "Alisin ang mga alpombra na madaling gumalaw para maiwasan ang pagkadulas at pagka-hulog.";
"HomeMod.subHeaderThree" = "Pagligo/ Paliguan:";
"HomeMod.contentThree" = "Ang mga paliguan ay maaaring mas madaling galawan ng upuang-gulong sa pamamagitan ng pagpapalapad ng pintuan.  Ang mga inidoro sa banyo ay maaaring itaas para maging mas madali ang pagpunta at pag-alis ng inidoro;  isang toilet elevator na inilalagay sa pagitan ng sahig at inidoro;  isang seat elevator na inilalagay sa inidoro at nagtataas sa upuan; ang pagkabit ng toilet rails/ grab bars para mapabuti ang suporta paggalaw.";

