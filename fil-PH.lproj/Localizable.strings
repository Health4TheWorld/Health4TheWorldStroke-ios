/* FAQ */
"Faq.questionOne" = "Ano ang Stroke?";
"Faq.answerOne" = "Sa stroke, naharang o pumuputok ang blood vessel na nagsu-supply sa partikular na bahagi ng utak. Nagreresulta ito ng pagkamatay ng cell sa apektadong bahagi ng utak. Maaaring pansamantala o habambuhay na pagkawala ng paggalaw ng isa o higit pang parte ng katawan.";
"Faq.questionTwo" = "Sa iyong palagay, ano ang dapat gawin sa nakararanas ng stroke?";
"Faq.answerTwo" = "Tumawag agad ng medikal na tulong sa isang propesyonal (doktor o nars).";
"Faq.questionThree" = "Sa iyong palagay, baket kailangang umaksiyon agad?";
"Faq.answerThree" = "S. Maaaring maiwasan ang panganib na magkaroon ng kapansanan o mahantong sa pagkamatay kung magamot agad ito.";

/* BLOOD PRESSURE */
"BloodPressure.problem" = "Maaaring maging sanhi ng stroke ang mataas na presyon ng dugo na makakapinsala sa mga ugat sa buong katawan na nagiging sanhi ng madaling  pagputok o  pagbara.";
"BloodPressure.problemBoldPart" = "Mataas na presyon ng dugo";
"BloodPressure.problemNonBoldPart" = " can cause strokes by damaging arteries throughout the body, causing them to burst or clog more easily.";
"BloodPressure.whatToDo" = "Sa pangkalahatan populasyon na may edad na 60 o higit pa, ang paggamot sa mataas na BP para mapababa ito sa 150/90 mm HG ay makakabawas sa stroke.";
"BloodPressure.whatToDoBoldPart" = "lower than 150/90 mm Hg";

/* HEART RATE */
"HeartRate.problem" = "Atrial Fibrillation, o iregular na pintig ng puso, ay isang mapanganib na dahilan ng stroke.";
"HeartRate.problemBoldPart" = "Atrial Fibrillation";
"HeartRate.whatToDo" = "Kung nakakaramdam ka na ng iregular na pagtibok ng iyong puso, tawagan agad ang iyong doktor para magpatingin,  kasama ang electrocardiogram (ECG/EKG). Batay sa iyong nakalipas na rekord ng kalusugan at sa panganib na ma-stroke sa hinaharap, maraming mga mapagpipilian sa paggamot tulad ng anticoagulants, na makakatulong sa pamamahala sa Afib.";
"HeartRate.whatToDoFirstBoldPart" = "electrocardiogram";
"HeartRate.whatToDoSecondBoldPart" = "anticoagulants";

/* BLOOD SUGAR */
"BloodSugar.problem" = "Ang Labis na asukal sa dugo ay maaaring makadagdag sa mapanganib ng namumuong stroke at sakit sa puso.  Ang pagsusuri sa dugo na tinatawag na HbA1c ay nagbibigay ng hitsura ng iyong katamtamang kontrol ng blood sugar para sa nakalipas na 2 hanggang 3 buwan.";
"BloodSugar.problemBoldPart" = "High blood sugar";
"BloodSugar.whatToDo" = "Nirerekomenda ng  Samahan ng American Heart na panatilihin ang antas ng blood sugar (Hgb1A1c) mababa sa 6% para sa mga pasyente na mayroong pre-diabetes at mababa sa 7% para sa mga pasyenteng diabetic.";
"BloodSugar.whatToDoFirstBoldPart" = "below 6%";
"BloodSugar.whatToDoSecondBoldPart" = "below 7%";

/* DIET */
"Diet.problem" = "Mahalagang mapanatili ng mga pasyenteng na-stroke ang malusog na pamumuhay. Nanganganib naman sa kakulangan sa calcium at vitamin D ang mga pasyenteng na-stroke na matagal na nananatili sa isang pasilidad sa pangngalaga.";
"Diet.whatToDo" = "Higit na makabubuti ang pagkain ng mga masustansiyang pagkain at may kasamang iba’t ibang prutas at gulay, whole grain, mga pagkaing low-fat, karneng walang taba at isdang walang balat, at mga mani at legumes. Bawasan ang  purong taba, trans fat, sodium, pulang karne, matatamis na pagkain at matatamis na inumin.";

/* BLADDER/BOWEL */
"BladderBowel.problem" = "Maakararanas ang ibang pasyente ng di-mapigilan ang pag-ihi o pagkalimot.";
"BladderBowel.whatToDoBulletOne" = "Tasahin ang hindi pag-ihi sa pamamagitan ng pag-scan sa pantog o paminsan-minsang catheterizations pagkatapos umihi, at itala ang dami nito.";
"BladderBowel.whatToDoBulletTwo" = "Magtungo ng regular sa banyo para masanay ang iyong pantog. Ang pag-ihi kada 2-3 oras ay makatutulong para maiwasan ang aksidente.";
"BladderBowel.whatToDoBulletThree" = "Uminom ng maraming tubig sa maghapon para manatiling hydrated at maiwasan ang hirap sa pagdumi.";

/* EXERCISES */
"Exercises.problem" = "Ang mga pasyenteng na stroke ay kadalasang may kahirapan sa pagpapanumbalik ng kanilang lakas at mga kakayahan.";
"Exercises.parOne" = "Stroke survivors should receive rehabilitation at an intensity that matches with anticipated benefit and tolerance.";
"Exercises.bulletOne" = "Para makatulong sa mayroong limitasyon sa paglakad: Kinailangan ng mga pasyente na magsagawa ng matindi, paulit-ulit, ibayong pagsasanay sa pagkilos sa ilalim ng pamamahala ng isang propesyonal na tagapangalaga ng kalusugan.";
"Exercises.bulletOneBoldPart" = "mayroong limitasyon sa paglakad";
"Exercises.bulletTwo" = "Para matulungan ang  mga pasyenteng natutumba kailangan silang magsanay sa pagbabalanse.";
"Exercises.bulletTwoBoldPart" = "natutumba";
"Exercises.bulletThree" = "Para makatulong habang nasa ospital at para sa mabilisang paggaling:  Inirerekomenda ng AHA/ASA ang mabagal na paglalakad, mga gawain sa pangangalaga sa sarili,  paminsan-minsang pag-upo at pagtayo, mga gawaing nakaupo, o iba’t ibang paggalaw. Dapat na ginagawa ang mga gawaing ito nang may pasensya kasama ang gabay na maabot ang 10 hanggang 20 tibok sa bawat minuto na pagtaas ng pintig ng puso, depende sa nirerekomenda ng iyong doktor.";
"Exercises.bulletThreeBoldPart" = "habang nasa ospital at para sa mabilisang paggaling";
"Exercises.bulletFour" = "Para matulungan ang mga pasyente sa gamutan o rehabilitasyon habang nasa loob at labas ng ospital: Inirerekomenda ng AHA/ASA ang mga aktibidad tulad ng paglalakad ng 3-5 araw kada linggo para sa 20-60 min/sesyon (o kasindami ng 10-min sesyon); pag-uunat ng 2-3 araw kada linggo para sa 10-30 segundo; Tai-Chi, yoga, at iba pang gawaing paglilibang bilang karagdagan sa  paglalakad o pag-uunat.";
"Exercises.bulletFourBoldPart" = "pasyente sa gamutan o rehabilitasyon habang nasa loob at labas ng ospital";
"Exercises.bulletFive" = "Para manatiling ligtas habang ginagawa ang pisikal na ehersisyo: Gumamit ng pantulong na kagamitan  para sa paglalakad at maingat na pag-eehersisyo, sa ilalim lamang ng pangangasiwa.";
"Exercises.bulletFiveBoldPart" = "ligtas habang ginagawa ang pisikal na ehersisyo";
"Exercises.bulletSix" = "Para mapanatili ang pisikal na kalusugan: Tiyakin sa inyong doktor  ang  ginawang plano ng aktibidad para sa pisikal na ehersisyo,  limitasyon ng tiyak na aktibidad, at ipinagbabawal na partisipasyon sa ehersisyo.";
"Exercises.bulletSixBoldPart" = "ang pisikal na kalusugan:";

/* SHOWER/BATH */
"ShowerBath.problem" = "Getting in and out of bathtubs/showers can be dangerous, and patients are at risk of falling.";
"ShowerBath.bulletOne" = "Pagligo>Planggana/Banyera:  Sa pangkalahatan mas ligtas ang pagligo gamit ang shower.";
"ShowerBath.bulletOneBoldPart" = "Pagligo>Planggana/Banyera:";
"ShowerBath.bulletTwo" = "Makatutulong  kung may makakapitan para mas madali ang pagkilos.";
"ShowerBath.bulletTwoBoldPart" = "Grab bars";
"ShowerBath.bulletThree" = "Bath mats with suction cups will help you avoid slipping.";
"ShowerBath.bulletThreeBoldPart" = "Bath mats";
"ShowerBath.paragraph" = "Siguraduhin na mayroong malapit na makakasamang maaaring tumulong kung kinakailangan. Hawakan ang bell o iba pang paraan para makatawag ng tulong. Ang pag-eensayo ng pagbalanse ay makatutulong sa taong may problema sa balanse.";

/* HOME MOD */
"HomeMod.problem" = "Dahil sa mararanasang limitadong pagkilos ng mga pasyente, mahalagang baguhin ang ayos ng inyong bahay para ito ay maging ligtas at madaling puntahan.";
"HomeMod.subHeaderOne" = "Silid-Tulugan:";
"HomeMod.contentOne" = "Para madaling puntahan ang silid-tulugan, maaari itong ayusin  sa pamamagitan ng pagtanggal sa mga nakaharang na bagay  para mabuksang mabuti  ang pintuan; lawakan ang daraanan; baligtarin ang bisagra para mabuksan ang pinto palabas sa halip na paloob. Kung kinakailangang nakasandal ang kama sa dingding, dapat na alam ito ng pasyente nang sa gayon ang apektadong parte ng katawan (mahinang parte) ay malapit sa dingding,  at ang malakas na parte ng katawan ang mahahawakan ng tutulong sa pagtayo ng pasyente mula sa higaan.";
"HomeMod.subHeaderTwo" = "Pangkalahatan:";
"HomeMod.contentTwo" = "Bawasan ang mga basahan para mas madaling makagalaw at maiwasan ang pagkadulas at pagkatumba.";
"HomeMod.subHeaderThree" = "Pagligo:";
"HomeMod.contentThree" = "Maaaring lawakan ang daraanan papunta sa banyo para magkasya at madaling makapasok ang wheelchair. Maaaring taasan ang upuan sa palikuran para mas madali itong maabot:  ang pagpapataas sa palikuran na nakapatong  sa pagitan ng sahig at palikuran; ang nakataas na upuan na nakakabit sa bowl at umaangat sa upuan;  paglalagay ng mga rehas sa palikuran para mas matibay ang suporta sa pagkilos.";

