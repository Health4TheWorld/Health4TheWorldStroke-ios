/* FAQ */
"Faq.questionOne" = "स्ट्रोक (पक्ष्घात) के हो?";

"Faq.answerOne" = "स्ट्रोकमा, दिमाग को एक विशेष हिस्सामा रगत को आपूर्ति अवरुद्ध हुन्छ या कुनै मस्तिष्कमा रगत पुराउने रक्तनली फुटछ​ । यस्ले गर्दा त्येस् भागको मस्तिष्कको कोशहरुको मृत्यु हुन्छ । यस्ले गर्दा शरीरको एक वा बढी भागहरूको काममा अस्थायी वा स्थायी हानि हुन सक्छ।";

"Faq.questionTwo" = "कसैलाई पक्ष्घात भयको शँका तपाई भयो भने तपाई के गर्नु पर्छ​ ?";


"Faq.answerTwo" = " चिकित्सा सहायता (डाक्टर वा नर्स) को लागी चाँडै कल गर्नुहोस्।";

"Faq.questionThree" = "तपाईंले किन चाँडैनै कार्य गर्न आवश्यक छ?";

"Faq.answerThree" = "चाँडैनै  मेडिकल उपचार गर्नाले पछ्घातबाट हुने अपांगता वा मृत्यु बाट बच्न सकिन्छ​।";

/* BLOOD PRESSURE */
"BloodPressure.problem" = "उच्च रक्तचापले शरीरको सबैतिरको धमनी बिगार्दछ जसले गर्दा ती धमनीहरुमा रगत सजिलै जम्दछ या फ़ुट्दछ​ र  स्ट्रोक (पक्ष्घात) गराउँदछ​ ।";

"BloodPressure.problemBoldPart" = "उच्च रक्तचाप";

"BloodPressure.problemNonBoldPart" = "उच्च रक्तचापले शरीरको सबैतिरको धमनी बिगार्दछ जसले गर्दा ती धमनीहरुमा रगत सजिलै जम्दछ या फ़ुट्दछ​ र  स्ट्रोक (पक्ष्घात) गराउँदछ​।";
"BloodPressure.whatToDo" = "सामान्यता ६० वर्ष वा माथिका उमेरका ब्यक्तिहरुलाई रक्तचाप (BP) 150/90 mmHg भन्दा कम राख्ने लक्ष्यगरी उपचार गरे स्ट्रोक (पक्ष्घात) घटदछ​ ।";

"BloodPressure.whatToDoBoldPart" = "150/ 90 मिमी एच.जी. भन्दा कम";

/* HEART RATE */
"HeartRate.problem" = "एट्रियल फ़ीब्रिलेशन्, अथार्थ​ अनियमित हृदयदर, स्ट्रोक को लागी एक जोखिम कारक हो।";

"HeartRate.problemBoldPart" = "एट्रियल फ़ीब्रिलेशन्";

"HeartRate.whatToDo" = "यदि तपाईंलाई अनियमित हृदयदरको शंका छ भने, परीक्षण को लागि तुरुन्तै तपाईंको डाक्टर संग संपर्क गर्नुहोस् , जसमा इलेक्ट्रोकार्डियोयोग्राम (ईसीजी / ईकेजी) हुन सक्छ। तपाईंको मेडिकल हिस्टोरीको आधार र​ भविष्यमा तपाईंलाई स्ट्रोक हुनसक्ने जोखिमको आधारमा, औषधिका धेरै विकल्पहरू हुनसक्छन्, जस्तै anticoagulants, जसले एट्रियल फ़ीब्रिलेशन् (AFib) व्यवस्थापन गर्न मद्दत गर्नेछ।";

"HeartRate.whatToDoFirstBoldPart" = "इलेक्ट्रोकार्डियोग्राम";


"HeartRate.whatToDoSecondBoldPart" = "anticoagulants";

/* BLOOD SUGAR */
"BloodSugar.problem" = "रगतमा सुगरको मात्रा धेरै भयोभने स्ट्रोक र ​​हृदय रोग हुने सँम्भवना बड्न​ सक्छ। HbA1c भन्ने एउटा रगतको जाँचले ग​एको 2 देखि 3 महिना सम्मको तपाईंको औसत सुगरको जानकारी दिन्छ​ ।";
"BloodSugar.problemBoldPart" = "उच्च सुगरको स्तर ";

"BloodSugar.whatToDo" = "अमेरिकन हार्ट एसोसिएसनले सुगरको स्तर (HbA1C, एचबी ए1 सी) को 6% भन्दा कम पूर्व मधुमेह र 7% भन्दा कम मधुमेह रोगका लागि सिफारिस गर्दछ।";
"BloodSugar.whatToDoFirstBoldPart" = " 6% मुनि ";
"BloodSugar.whatToDoSecondBoldPart" = " 7% मुनि ";

///* DIET */
"Diet.problem" = "पक्ष्घातका रोगीहरूको लागि एक स्वस्थ जीवनशैली कायम राख्न महत्त्वपूर्ण छ। दीर्घकालीन हेरचाह सुविधाहरूमा रहने  पक्ष्घात रोगीहरूलाई क्याल्सियम र भिटामिन डी को कमी हुने जोखिम हुन्छ​।";

"Diet.whatToDo" = "फलफूल र सब्जीहरू, सम्पूर्ण अन्न, कम-मोटो डेरी उत्पादन, छाला रहित मासु र माछा, र नट र फलफुलको विभिन्न स्वस्थ आहार खानेकुरा राम्रो हुन्छ। संतृप्त बोसो, ट्रांस बोसो, नुन, रातो मासु, मिठाई र चिनी, गुलियो पेयहरु कम  गर्नुहोस।";

/* BLADDER/BOWEL */
"BladderBowel.problem" = "केही रोगीहरूलाई पिसाब फ़ेर्न नसक्ने अथवा पिसाब रोक्न नसक्ने हुन सक्दछ​।";

"BladderBowel.whatToDoBulletOne" = "पिसाब अडकिएमा, मुत्रथैलीको जाँच पिसाब फ़ेरेपछि ब्लडेर स्क्यानिङ वा intermittent catheterization द्वारा गर्नुहोस र पिसाब को मत्रा नाप्नुहोस् र रेकर्ड​ राखनुहोस्।";

"BladderBowel.whatToDoBulletTwo" = "नियमित रूपमा समयमै बाथरूममा जानुहोस् तपाईंको मूत्राशयलाई प्रशिक्षण दिन मद्दत गर्नुहोस्। हरेक 2-3 घण्टा पिसाब फेर्नाले दुर्घटनाहरू रोक्न मद्दत गर्न सक्छ।";

"BladderBowel.whatToDoBulletThree" = "शरीरमा पानीको मत्रा ठिक राख्न​ दिनको बेलामा धेरै झोल पदार्थ पिउनुहोस जसले कब्जियत बाट बच्न मद्दत पुर्याउछ ।";


/* EXERCISES */
"Exercises.problem" = "पक्ष्घातका रोगीहरूले पहिलेकै शारीरिक शक्ति र क्षमताहरू प्राप्त गर्न अक्सर कठिनाइहरू भोग्ने गर्दछन्।";
"Exercises.parOne" = "पक्ष्घात भएका व्यक्तिहरुले पुनर्वास प्राप्त गर्नुपर्दछ जुन प्रत्याशित लाभ र सहनशीलता संग मेल खान्छ ।";
"Exercises.bulletOne" = "हिड्न गर्होको सहयोग पाउन : रोगीहरूले स्वास्थ्य सम्ब्न्धी पेशेवरको निरीक्षणमा गहन, दोहोरो, गतिशीलता-कार्य प्रशिक्षण गर्नु पर्छ।";
"Exercises.bulletOneBoldPart" = "हिड्न गर्हो";
"Exercises.bulletTwo" = "लड्न लागेको बेला मद्दतको लागि: बिरामीले सन्तुलनको प्रशिक्षण लिनुपर्छ।";
"Exercises.bulletTwoBoldPart" = "लड्ने :";
"Exercises.bulletThree" = "अस्पतालम़ा भर्ना हुदा वा चाडैनै निको हुन मद्दत गर्नको लागि: AHA / ASA ले निम्न स्तरको हिड्ने , स्वयं हेरविचार गतिविधि, बेला-बेलामा बस्ने वा खडा हुने, बसेर गर्ने  गतिविधिहरू, वा चलेर​ गर्ने गतिविधिहरूको सिफारिस गर्छ । यी क्रियाकलापहरू आफ़ुले सहन सक्नेगरी दिशानिर्देश अनुसार र आफ़्नो चिकित्सकको सल्लाह बमोजिम हृदय रेटमा 10 देखि 20 बीट प्रति मिनेट बडी प्राप्त गर्ने गरी गर्न सकिन्छ।";
"Exercises.bulletThreeBoldPart" = "अस्पताल भर्ना र चाडै निको हुने";
"Exercises.bulletFour" = "इनपेटिन्ट र आउटपेन्टेंट व्यायाम थेरेपी वा पुनर्वासको समयमा मद्दत गर्न: AHA/ASA सिफारिश अनुसार ३-५ दिन हप्ताको 20-60 मिनेट / सत्र (वा धेरै 10-मिनेट सत्र) हिड्ने, तन्किने २-३ दिन प्रति हप्ता 10-30 सेकेण्ड; ताई ची, योग, र मनोरञ्जन गतिविधिहरू हिंड्ने वा तन्किने";
"Exercises.bulletFourBoldPart" = "अस्पताल भित्रको बिरामी र बहिरको बिरामी लाई  व्यायाम थेरेपी वा पुनर्वास";
"Exercises.bulletFive" = "शारीरिक व्यायामको समयमा सुरक्षित रहन: हिड्न र ध्यानपूर्वक अभ्यास गर्न सहायक यन्त्रहरू प्रयोग गर्नुहोस्, केवल अरुको पर्यवेक्षणमा।";
