///* FAQ */
"Faq.questionOne" = "Kiharusi ni nini?";
"Faq.answerOne" = "Katika Ugonjwa wa Kiharusi , utoaji wa damu sehemu fulani ya ubongo imezuiwa au mshipa wa damuamabo hutoa damau sehemu fulanihupasuka. Hii husababisha kifo cha seli katika sehemu iliyoathirika ya ubongo.Hii inaweza kusababisha hasara ya muda au ya kudumu ya kazi kupotea katika sehemu moja au zaidi ya mwili.";
"Faq.questionTwo" = "Unapaswa kufanya nini ikiwa unadhani mtu ana kiharusi?";
"Faq.answerTwo" = "Piga simu kwa msaada wa kitaalamu wa matibabu (daktari au muuguzi) mara moja.";
"Faq.questionThree" = "Kwa nini unahitaji kuchukua hatua haraka?";
"Faq.answerThree" = "Kupata matibabu ya haraka hupunguza hatari ya ulemavu au hata kifo.";

/* BLOOD PRESSURE */
"BloodPressure.problem" = "Shinikizo la damu huweza kusababisha kiharusi kwa kuharibu mishipa ya damu katika mwili, na kuifanya kupasuka au kufungwa kwa urahisi.";
"BloodPressure.problemBoldPart" = "Shinikizo la damu";
"BloodPressure.problemNonBoldPart" = "huweza kusababisha kiharusi kwa kuharibu mishipa ya damu katika mwili, na kuifanya kupasuka au kufungwa kwa urahisi.";
"BloodPressure.whatToDo" = "Kwa idadi ya watu wenye miaka 60 au zaidi, kutibu shinikizo la damu kwa lengo la chini kuliko 150/90 mmHg  inapunguza kiharusi.";
"BloodPressure.whatToDoBoldPart" = "Chini ya 150/90 mm Hg";

/* HEART RATE */
"HeartRate.problem" = "Kiwango cha moyo kisicho cha kawaida, ni hatari ya kiharusi.";
"HeartRate.problemBoldPart" = "Moyo kupiga kwa kiwango kisicho cha kawaida";
"HeartRate.whatToDo" = "Ikiwa una shaka ya kiwango cha moyo kisicho cha kawaida, wasiliana na daktari wako mara moja kwa ajili ya kupimwa, kipimo kama cha kiwango cha moyo (ECG/EKG). Kulingana na historia yako ya matibabu ya zamani na hatari yako ya kuwa na kiharusi baadaye kuna madawa kadhaa, kama vile madawa ya kufanya damu nyembamba, ambayo yatasaidia kutibu kiwango cha moyo kisicho cha kawaida.";
"HeartRate.whatToDoFirstBoldPart" = "Kipimo cha kiwango cha moyo kisicho  cha kwaida";
"HeartRate.whatToDoSecondBoldPart" = "madawa ya kufanya damu nyembamba";

/* BLOOD SUGAR */
"BloodSugar.problem" = "Sukari ya damu kuwa juu inaweza kuongeza hatari ya kupatwa na ugonjwa wa kiharusi na ugonjwa wa moyo. Kipimo cha damu kichoitwa HbA1c kinatoa picha ya kudhibiti sukari yako ya damu kwa miezi2 au 3 iliyopita.";
"BloodSugar.problemBoldPart" = "Sukari ya damu kuwa juu";
"BloodSugar.whatToDo" = "Shirika la moyo la Marekani linapendekeza kuweka kiwango cha sukari ya damu(Hgb1A1c) chini ya 6% kwa wagonjwa kabla ya kupata kisukari na chini ya 7% kwa wagonjwa wa kisukari.";
"BloodSugar.whatToDoFirstBoldPart" = "Chini ya 6%";
"BloodSugar.whatToDoSecondBoldPart" = "Chini ya 7%";

/* DIET */
"Diet.problem" = "Ni muhimu kwa wagonjwa wa kiharusi kudumisha maisha ya afya bora. Wagonjwa wa kiharusi wanoishi katika vituo vya huduma vya muda mrefu wana hatari ya kukosa Kalsiamu na Vitamini D.";
"Diet.whatToDo" = "Ni bora kula chakula cha afya kama aina mbalimbali za matunda na mboga, nafaka, bidhaa za maziwa zilizo na kiwango cha chini cha mafuta, nyama isiyo na ngozi na samaki, njugu na mboga.Punguza kiwango cha mafuta, sodiamu, nyama nyekundu, permende na vinywaji vitamu vilivyoongezwa sukari.";

///* BLADDER/BOWEL */
"BladderBowel.problem" = "Wagonjwa wengine watajiskia kutokwa na mkojo bila hiari au uhifadhi wa mkojo.";
"BladderBowel.whatToDoBulletOne" = "Tathmini uhifadhi wa mkojo kwa njia ya kupiga picha kibofu au kuwekwa mpira wa kukojoa baada ya kurisha na kurekodi kiwango cha mkojo.";
"BladderBowel.whatToDoBulletTwo" = "Nenda kwa choo kwa nyakati za kawaida ili kusaidia kufanyisha kibofu mazoezi. Kurisha kila baada ya masaa  2-3 hours kunaweza kuzuia ajali ya mkojo kutoka bila hiari.";
"BladderBowel.whatToDoBulletThree" = "Kunywa maji mengi wakati wa mchana ili kukusaidia kuzuia kuvimbiwa.";

/* EXERCISES */
"Exercises.problem" = "Wagonjwa wa kiharusi huwa na shida ya kurejea nguvu za kimwili na uwezo.";
"Exercises.parOne" = "Waathirika wa kiharusi wanapaswa kupokea ukarabati wa kimwili kwa kinachofanana na manufaa na uvumilivu.";
"Exercises.bulletOne" = "Kusaidia na upungufu wa namna ya kutembea: Wagonjwa  wanapaswa kufanya mazoezi makubwa, ya kurudia na uhamiaji chini ya usimammizi wa  mtaalamu wa huduma za afya.";
"Exercises.bulletOneBoldPart" = "Upungufu wa namna za kutembea";
"Exercises.bulletTwo" = "Ili kusaidia na kuanguka : Wagonjwa wanapaswa kufanya mazoezi ya usawa.";
"Exercises.bulletTwoBoldPart" = "Kuanguka:";
"Exercises.bulletThree" = "Kusaidia wakati wa kulala hospitalini na kupona mapema: AHA/ASA inapendekeza kutembea kidogo, shughuki za kujitegemea, kuketi au kusimama kwa muda, shughuli za kuketi au shughuli za mwendo.Shughuli hizi zinapaswa kufanywa kulingana na kiwango cha uvumilivu, na mwongozo wa kufikia ongezeka la 10-20 kwa kiwango cha moyo kila dakika, kulingana na amri ya daktari wako.";
"Exercises.bulletThreeBoldPart" = "Wakati wa kulala hospitalini na kupona mapema";
"Exercises.bulletFour" = "Kusaidia wakati wa tiba ya mazoezi au ukarabati wa mwili: AHA/ASA inapendekeza shughuli kama kutembea kwa dakika 20-60 kwa muda mara 3-5 kwa wiki (per week for 20-60 min/session (au vikao vingi vya dakika 10); mazoezi ya kunyoosha siku 2 kwa wiki kwa sekunde 10 kila mara; Tai chi, yoga, na shughuliza burudani zinazosaidia kutembea na kujinyoosha.";
"Exercises.bulletFourBoldPart" = "Wagonjwa kutumia tiba ya mazoezi au ukarabati wa kimwili";
"Exercises.bulletFive" = "Kubaki salama wakati wa mazoezi ya kimwili: Tumia vifaa vya kusaidia kwa kutembea na kufanya mazoezi kwa makini, tu chini ya usimamizi.";
"Exercises.bulletFiveBoldPart" = "Kubaki salama wakati wa mazoezi ya kimwili";
"Exercises.bulletSix" = "Kudumisha afya bora ya kimwili: Uliza dakatari wako juu mazoezi ya upendeleo,mapungufu ya shughuli maalum, na vikwazo vya ushiriki wa mazoezi.";
"Exercises.bulletSixBoldPart" = "Afya ya kimwili:";

/* SHOWER/BATH */
"ShowerBath.problem" = "Kuingia na kutoka bafuni inaweza kuwa hatari, na wagonjwa wako katika hatari ya kuanguka.";
"ShowerBath.bulletOne" = "Mfereji wa kuogea >hodhi: Mifereji ya kuogea ni salama kuliko hodhi kwa ujumla.";
"ShowerBath.bulletOneBoldPart" = "Mfereji wa kuogea>hodhi:";
"ShowerBath.bulletTwo" = "Vizuizi vya kushikilia vinaweza kufanya harakati iwe rahisi.";
"ShowerBath.bulletTwoBoldPart" = "Vizuizi vya kushikilia";
"ShowerBath.bulletThree" = "Mikeka ya kuoga iliyo na vikombe vya kumumunya inaweza kuzuia kuanguka.";
"ShowerBath.bulletThreeBoldPart" = "Mikeka ya kuoga";
"ShowerBath.paragraph" = "Hakikisha kuna mtu yuko karibu ili kukusaidia ikiwa unahitaji usaidizi. Weka kengele karibu au kuwa na njia nyingine ya kuitisha msaada. Mazoezi ya usawa inaweza kusaidia watu wenye matatizo ya usawa.";

/* HOME MOD */
"HomeMod.problem" = "Kwa sabau wagonjwa wanaweza kupata shida ya kutembea, ni muhimu kurekebisha nyumba yako ili iwe salama na rahisi kupatikana.";
"HomeMod.subHeaderOne" = "Chumba cha kulala:";
"HomeMod.contentOne" = "Upatikanaji wa chumba cha kulala unaweza kuboreshwa kwa kusongesha vitu ili mlango ufunguke kikamilifu; kupanua milango; kugeuza bawaba ili mlango ufunguke na nje badala ya ndani. Kama kitanda kinapaswa kuwa karibu na ukuta , kinapsawa kuelelezwa kama upande wa mgonjwa ulioadhirika(upande dhaifu) uko karibu na ukuta, ili upande ulio na nguvu unaweza kumsaidia mgonjwa kutoka kwa kitanda.";
"HomeMod.subHeaderTwo" = "Jumla :";
"HomeMod.contentTwo" = "Ondoa mikeka ambayo inaweza kusonga kwa urahisis ili kuzuia kuteleza na kuanguka.";
"HomeMod.subHeaderThree" = "Kuoga:";
"HomeMod.contentThree" = "Vyumba vya bafu vinaweza kufikika kwa urahisi zaidi kwa viti vya gurudumu kwa kupanua milango. Viti vya choo vinaweza kuinuliwa ili kupunguza changamoto ya kuingia na kutoka kwenye choo: Lifti ya choo ambayo inawekwa katika ya sakafu na choo; Lifti ya kiti ambayo inaunganishwa na bakuli ya choo na inainua kiti; kuweka reli za choo na vizuizi vya kushikilia ili kusaidia uhamaji.";

